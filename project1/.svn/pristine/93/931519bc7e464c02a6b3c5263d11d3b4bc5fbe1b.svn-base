.tech table{width:100%;}
.tech thead tr{height:43px; box-sizing:border-box; background: #eff3f6; color:#000; font-size:15px;}
.tech thead tr th{border-top:2px solid #093e95; border-bottom:1px solid #e5e5e5; box-sizing: border-box; padding: 0 10px; }
.tech tbody tr{height:41px; box-sizing:border-box;}
.tech tbody tr td{border-bottom:1px solid #e5e5e5; text-align:left; font-size:14px;}
.tech{position:relative; text-align:center;}




/* 페이징 */
.paging{margin-top:26px; text-align: center;}
.paging a {display: inline-block; color: #333; font-size: 12px; text-decoration: none; margin: 0 -1px; border: solid 1px #cccfd0; padding: 6px 10px 6px 11px;}
.paging a.active, .paging a:hover {color: #fff; background: #093e95; border: 1px  solid #093e95; font-weight:normal;}
a.page_prev, .paging a.page_next {width:29px; height:32px; overflow: hidden; text-indent: -999px; background: url("http://html.designclick.co.kr/404.html") 0 0 no-repeat; box-sizing: border-box; border: 0; margin-bottom: -12px;}
a.page_prev{margin-right:10px;}
a.page_next{margin-left:10px;}
.paging a.page_prev:hover {background: url("http://html.designclick.co.kr/404.html") 0 0 no-repeat; background-position:0 -42px; }
.paging a.page_next {background: url("http://html.designclick.co.kr/404.html") 0 0 no-repeat; background-position:-45px 0; }
.paging a.page_next:hover {background: url("http://html.designclick.co.kr/404.html") 0 0 no-repeat; background-position:-45px -42px; }




/* 검색 */

.search{left: 0; bottom:0px;padding-bottom:41px;text-align:left;}
.search dt{float:left;}
.search dd{float:left; margin-left: 10px;}
.search select{height:25px; margin-right:10px;}
.search_i{width:150px; height:25px; background:#f7f7f7; border:1px solid #c4c4c4;}
.submit_i{width:61px; height:25px; display:inline-block; box-sizing:border-box; background:#3d3d3d; border:0; border-radius:2px; color:#fff; line-height:25px; padding-left:20px;}



/* 글쓰기 */

.write {1text-align:right; margin-top:20px; right: 0;}
.write_btn {color: #fff; text-align:center; background: #093e95; font-weight:700;; display:inline-block; width: 84px; height: 41px; box-sizing: border-box; line-height:41px; border-radius:3px;}
.write_btn2 {color: #fff; text-align:center; background: #093e95; font-weight:700;; display:inline-block; width: 84px; height: 41px; box-sizing: border-box; line-height:41px; border-radius:3px;}



