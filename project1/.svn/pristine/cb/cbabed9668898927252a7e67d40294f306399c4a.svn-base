.wrap {
  height: 100%;
  width: 100%;
  box-sizing: border-box;
  margin: 0 auto;
}


/* Header Title */
.header {
  padding: 30px;
  text-align: center;
  background: #fff;
}

/* Style the top navigation bar */
.topnav {
  overflow: hidden;
  background-color: #3e4149;
}
.topnav ul {
  padding-left: 0px;
  margin: 0;
}

.topnav ul li {
  list-style:none;
  display: inline;
}
/* Style the topnav links */
.topnav ul li a {
  float: left;
  display: block;
  color: #cadefc;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
}

/* Change color on hover */
.topnav ul li a:hover {
  background-color: #cca8e9;
  color: #3e4149;
}

/* Style the side navigation */
.sidenav {
  height: 100%;
  width: 200px;
  position: fixed;
  z-index: 1;
  background-color: #ddd;
  overflow-x: hidden;
}
.sidenav ul {
  padding-left: 0px;
}

.sidenav ul li {
  list-style:none;
}

/* Side navigation links */
.sidenav ul li a {
  color: #3e4149;
  padding: 5px 16px 5px 16px;
  text-decoration: none;
  display: block;
  font-size: 18px;
}

.sidenav ul li a img {
     width: 20px;
     margin-right: 5px;
}

.sidenav ul li ul li a {
  color: #3e4149;
  padding: 5px 16px 5px 32px;
  text-decoration: none;
  display: block;
  font-size: 15px;
}

/* Change color on hover */
#click a:hover {
  background-color: #cadefc;
  color: #3e4149; 
}


/* Style the content */
.content {
  margin-left: 200px;
  padding-left: 20px;
}

/* Style the hr */
hr {
    border: 0;
    height: 0;
    border-top: 1px solid rgba(0, 0, 0, 0.1);
    border-bottom: 1px solid rgba(255, 255, 255, 0.3);
}

/* Style the content */
#content {
  margin-left: 10px;
}

/* Style the notice */
.input {
    border-collapse: collapse;
    text-align: left;
    line-height: 1.5;
    margin : 20px 10px;
}

