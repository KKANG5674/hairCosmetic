#optionArea{
    line-height: 1.5;
    background-color: #fafafa;
}
#optionArea th {
	height: 30px;
    vertical-align: top;
    background-color: #cadefc;
}
#optionArea select, #optionArea input {
	height: 25px;
}
#optionArea th, #optionArea td {
    padding: 9px 9px 7px;
    border: 1px solid #cadefc;
    text-align: left;
}
/* 버튼 */
#select {
	text-align: center;
	border: 1px solid #cadefc;
	background: #cadefc;
	color: #3e4149;
}
/* 회원목록 */
#optionArea2 {
    line-height: 1.5;
    background-color: #fafafa;
}
#optionArea2 th, #optionArea2 td {
    padding: 9px 9px 7px;
    border: 1px solid #cadefc;
    font-weight: normal;
}
#optionArea2 tr:first-child{
	padding: 9px 9px 7px;
    border: 1px solid #cadefc;
    text-align: center;
    background-color: #cadefc;
    font-weight: bold;
}
/* 버튼 */
#update, #delete {
    text-align: center;
    border: 1px solid #cadefc;
    background: #cadefc;
    color: #3e4149;
}

/* Style the notice_admin_input */
#notice_input {
	text-align: center;
	width: 80%;
    line-height: 1.5;
    background-color: #fafafa;
}
#notice_input th {
    height: 30px;
    vertical-align: top;
    background-color: #cadefc;
}
#notice_input th, #notice_input td {
    padding: 9px 9px 7px;
    border: 1px solid #cadefc;
    text-align: left;
}

/* 버튼 */
#insert {
	height: 30px;
    text-align: center;
    border: 1px solid #cadefc;
    background: #cadefc;
    color: #3e4149;
}
#cancel {
	height: 30px;
    text-align: center;
    border: 1px solid #cadefc;
    background: #ffffff;
    color: #3e4149;
}

/* Responsive layout - when the screen is less than 800px wide, make the two columns stack on top of each other instead of next to each other */
@media screen and (max-width: 800px) {
  .leftcolumn, .rightcolumn {   
    width: 100%;
    padding: 0;
  }
}

/* Responsive layout - when the screen is less than 400px wide, make the navigation links stack on top of each other instead of next to each other */
@media screen and (max-width: 400px) {
  .topnav a {
    float: none;
    width: 100%;
  }
}
