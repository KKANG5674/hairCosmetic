function point_check_bak() {
	var frm = document.morning;
	var user_point = frm.ps_user_point.value;
	var use_point = frm.use_buyer_point.value;
	var total_price = frm.ps_total_price.value;
	var total_coupon = frm.basic_coupon_price.value;
	var total_account_price_stmp = total_account_point_stmp = "";
	if(isNaN(use_point)) {
		alert("사용 적립금는 숫자만 입력하여 주십시오.");
		frm.use_buyer_point.value = 0;
		frm.remain_buyer_point.value = eval(user_point);
		frm.use_buyer_point.focus();
		return;
	}

	if(frm.ps_user_point.value == "") user_point = 0;
	else user_point = parseInt(frm.ps_user_point.value);

	if(frm.use_buyer_point.value == "") use_point = 0;
	else use_point = parseInt(frm.use_buyer_point.value);

	if(frm.ps_total_price.value == "") total_price = 0;
	else total_price = parseInt(frm.ps_total_price.value);

	if(frm.basic_coupon_price.value == "") total_coupon = 0;
	else total_coupon = parseInt(frm.basic_coupon_price.value);

	if(user_point < use_point) {
		alert("사용 가능한 적립금 ["+user_point+"] 보다 많은 적립금을 사용 하실수 없습니다.");
		frm.use_buyer_point.value = 0;
		frm.remain_buyer_point.value = eval(user_point);
		frm.use_buyer_point.focus();
		total_account_price_stmp = number_format2(eval(total_price-total_coupon)) + ' 원';
	} else if((total_price-total_coupon) < use_point) {
		alert("결제 금액 보다 많은 적립금을 입력 하셨습니다.");
		frm.use_buyer_point.value = 0;
		frm.remain_buyer_point.value = eval(user_point);
		frm.use_buyer_point.focus();
		total_account_price_stmp = number_format2(eval(total_price-total_coupon)) + ' 원';
	} else {
		total_account_price_stmp = number_format2(eval(total_price-total_coupon-use_point)) + ' 원';
	}
	
	total_account_point_stmp = number_format2(eval(frm.use_buyer_point.value)+'');
	$("#total_account_price_tmp").html(total_account_price_stmp);
	$("#total_account_price_tmp2").html(total_account_price_stmp);
	$("#total_account_point_tmp2").html(total_account_point_stmp);
	
	return;
}


