				$(document).ready(function(){ 

					$("input#phone").blur(function(){
						var num = $("#phone").val();
						blur(num)
					});

					$("input#phone").click(function(){

						var num = $("#phone").val();
						focus(num);
					});
				});


				function focus(num) {

					num = num.replace(/[^0-9]/g, '');
					$("#phone").val(num);
				}

				function blur(num) {

					num = num.replace(/[^0-9]/g, '');
					var tmp = '';
					tmp += num.substr(0, 3);
					tmp += '-';
					tmp += num.substr(3, 4);
					tmp += '-';
					tmp += num.substr(7);
					$("#phone").val(tmp);
				}
