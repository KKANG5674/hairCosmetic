		$("#noCash").css('border-color','white');
		$("#bossCash").css('border-color','#E95CE6');
		$("#personCash").css('border-color','white');
		
	} else if (smode == "poss") {
		$("#noCash").css('border-color','white');
		$("#bossCash").css('border-color','white');
		$("#personCash").css('border-color','#E95CE6');
	}
	
	
	
	if(cmode == "no") {
		$(".cash_show_t").hide();
		$(".cash_show_cc").hide();
		$(".cash_show_cp").hide();
		
		
		
	
	} else if(cmode == "vat") {
		$(".cash_show_t").show();
