	}
}

function point_check() {
    var m = document.morning;
    var user_point = m.ps_user_point.value;
    var use_point = m.use_buyer_point.value;
    var total_price = m.ps_total_price.value;
	var pay_price = m.ps_pay_price.value;
//	if (user_point=="")
//	{
//		user_point = 0;
//	}
//	if (use_point=="")
//	{
//		use_point = 0;
//	}
//	if (total_price=="")
//	{
//		total_price = 0;
//	}
//	if (pay_price=="")
//	{
//		pay_price = 0;
//	}
	var delivery_price = 0;
	var delivery_max_price = 0;
	var coupon_price = 0;
	var wrapping_price = 0;
	var delivery_data = "";
	var wrapping_data = "";
	var temparray1 = new Array();
	var temparray2 = new Array();
	var temparray3 = new Array();
	var remain_point = 0;
	var account_price = 0;

	delivery_data = fun_radio_val(m.delivery_method);

	if(delivery_data != "") {
		temparray1 = delivery_data.split(",");
		delivery_price = temparray1[1];
		delivery_max_price = temparray1[2];
	} else {
		//alert('배송료를 선택하셔야 합니다.');
		delivery_price = "0";
		delivery_max_price = "0";
	}
/*	
	wrapping_data = fun_radio_val(m.wrapping_method);
	if(wrapping_data != "") {
		temparray2 = wrapping_data.split(",");
		wrapping_price = temparray2[1];
		wrapping_max_price = temparray2[2];
	} else {
		wrapping_price = "0";
		wrapping_max_price = "0";
	}
*/	
	if(eval(total_price) >= eval(delivery_max_price)) { //배송비 설정(상품가격대비 가격설정)
		delivery_price  = 0;	
	}

	if(m.basic_delivery_price2.value == "free") { 	//배송비 설정(무료상품대비 가격설정)
		delivery_price = 0;
	}
	
//	if(m.is_use_coupon.value == "Y") {
//		if(m.sel_coupon.value != "") {
//			temparray3 = m.sel_coupon.value.split(";");
//			coupon_price = temparray3[0];
//		}
//	}

    if(isNaN(use_point)) {
        alert("사용 포인트는 숫자만 입력하여 주십시오.");
        m.use_buyer_point.value = "";
        m.remain_buyer_point.value = user_point;
    
	} else if(eval(user_point) < eval(use_point)) {
        alert("사용 가능한 포인트("+user_point+")보다 많은 포인트를 사용 하실수 없습니다.");
        m.use_buyer_point.value = "";
        m.remain_buyer_point.value = user_point;

    } else if((eval(pay_price) + eval(delivery_price) + eval(wrapping_price) - eval(coupon_price)) < eval(use_point)) {
        alert("결제 금액 보다 많은 포인트를 입력 하셨습니다.");
        m.use_buyer_point.value = "";
        m.remain_buyer_point.value = user_point;
    }

	//남은 포인트 체크
	if(m.use_buyer_point.value=="") {
		m.use_buyer_point.value = "";
	}else{

		use_point = m.use_buyer_point.value;
		remain_point = eval(user_point) - eval(use_point);
	}

	//총가격은 배송비,포장비,포인트사용비로 결정됩니다.
	account_price = eval(pay_price) + eval(delivery_price) + eval(wrapping_price) - eval(coupon_price) - eval(use_point);
    m.total_account_price.value = eval(account_price);
    m.remain_buyer_point.value = eval(remain_point);
	total_account_price_stmp = number_format2(eval(account_price)) + ' 원';
	total_account_price_stmp2 = number_format2(eval(use_point)) + ' 원';
    total_account_price_tmp.innerHTML = total_account_price_stmp;
	total_account_price_tmp2.innerHTML = total_account_price_stmp2;
	total_account_price_tmp3.innerHTML = total_account_price_stmp2;

	document.ini.P_AMT.value = account_price;
	document.SendPayForm_id.price.value = account_price;
	document.morning.m_price.value = account_price;
}

function fn_coupon_price(obj) {
	var frm = obj.form;
	var use_point = 0;
	if(frm.use_buyer_point.value != "") use_point = parseInt(frm.use_buyer_point.value);
	if(use_point > 0) {
		frm.use_buyer_point.value = "0";
		alert('사용하실 적립금을 다시 입력해 주세요.');
	}
	point_check();
	return ;
}


