function view_image(image_src){
	document.goods.main_img.src = image_src;
}

function fn_split_price(str) {
	var arr;
	var price = 0;
	if(str) {
		arr = str.split(",");
		if(arr[3]) price = parseInt(arr[3]);
	}

	return price;

}

function fn_price_total() {
	var frm = document.goods;
	var arr,val,nm;
	var price_total = price_unit = order_cnt = 0;
	
	if(frm.ps_num.value == "") {
		frm.ps_num.value = "1";
	}
	order_cnt = parseInt(frm.ps_num.value);
	
	if(frm.ps_old_price.value != "") {
		price_unit = parseInt(frm.ps_old_price.value);
	}

	for(var i=1; i <= 6 ; i++) {
		nm = eval("frm.foption_nm_"+i+".value");
		if(nm != "") {
			val = eval("frm.goods_option"+i+".value");
			price_unit += fn_split_price(val);
		}
	}
	price_total = price_unit*order_cnt;
	
	$("#print_total_price").html(fn_number_format(price_total+"")+" 원");
	$("input[name=price_total]").val(price_total);
	return ;
}

function fn_good_num(val) {
	var frm = document.goods;
	if (val > 0) {
		frm.ps_num.value = (frm.ps_num.value*1) + 1;
	} else {
		if(frm.ps_num.value > 1) {
			frm.ps_num.value = frm.ps_num.value - 1;
		}
	}
	fn_price_total();
	return ;
}


function fn_sendcart(c_flag) {

	var frm = document.goods;
	var nm,obj,err_cnt;
	err_cnt = 0;
	

//	if($('#datepicker').val() ==  '') {
//		alert('날짜를 선택해주세요!');
//		return;
//	}

	var goods_option10_val = $('input:radio[name=goods_option10]').is(':checked');
	

	if (goods_option10_val==false)
	{
		alert('제품구성을 선택하세요');
		return;
		return false;
	}

	if(c_flag=="S") {
		frm.ps_select.value = c_flag;
		frm.action = "";
		frm.submit();
	} else {
		frm.ps_select.value = c_flag;
		frm.action = "./order/orderPro.jsp";
		frm.submit();
	}
	return;

}

function menuclick( submenu ,cellbar) {
	if( old_menu != submenu ) {
		if( old_menu !='' ) {
			old_menu.style.display = 'none';
		}
		submenu.style.display = 'block';
		old_menu = submenu;
		old_cell = cellbar;
	} else {
		submenu.style.display = 'none';
		old_menu = '';
		old_cell = '';
	}
}

