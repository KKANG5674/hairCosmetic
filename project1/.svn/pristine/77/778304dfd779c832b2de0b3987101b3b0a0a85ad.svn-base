a {
	text-decoration:none;
	color: #191919;
}

a:focus, a:hover, a:active {
	text-decoration:none;
}


blockquote, q {
	quotes: none;
}


blockquote:before, blockquote:after,
q:before, q:after {
	content: '';
	content: none;
}

table {
	border-collapse: collapse;
	border-spacing: 0;
}

